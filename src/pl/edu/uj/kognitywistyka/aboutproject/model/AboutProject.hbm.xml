<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pl.edu.uj.kognitywistyka.aboutproject.model.AboutProject" table="k_O_Projekcie">
		<id name="aboutProjectId" column="Id">
			<generator class="increment"/>
		</id>
		<property name="description" column="Opis"/>
		<property name="date" column="Data"/>
   </class>
</hibernate-mapping>