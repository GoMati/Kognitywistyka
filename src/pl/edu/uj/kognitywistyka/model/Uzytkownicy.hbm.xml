<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pl.edu.uj.kognitywistyka.model.Uzytkownicy" table="k_Uzytkownicy">
		<id name="Id" column="Id">
			<generator class="increment"/>
		</id>
		<property name="Login" column="Login"/>
		<property name="Haslo" column="Haslo"/>
		<property name="Email" column="Email"/>
		<property name="Sol" column="Sol"/>
		<property name="DataRejestracji" column="DataRejestracji"/>
		<property name="Imie" column="Imie"/>
		<property name="Nazwisko" column="Nazwisko"/>
		<property name="TytulSpecjalizacja" column="TytulSpecjalizacja"/>
		<property name="NazwaUczelniLabor" column="NazwaUczelniLabor"/>
		<property name="Branza" column="Branza"/>
		<property name="Miejscowosc" column="Miejscowosc"/>
		<property name="Wojewodztwo" column="Wojewodztwo"/>
   </class>
</hibernate-mapping>