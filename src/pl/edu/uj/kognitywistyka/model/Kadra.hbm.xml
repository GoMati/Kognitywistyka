<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pl.edu.uj.kognitywistyka.model.Kadra" table="k_Kadra">
		<id name="Id" column="Id">
			<generator class="increment"/>
		</id>
		<property name="Imie" column="Imie"/>
		<property name="Nazwisko" column="Nazwisko"/>
		<property name="Tytul" column="Tytul"/>
		<property name="Opis" column="Opis"/>
		<property name="Zdjecie" column="Zdjecie"/>
		
		<many-to-one name="Stanowisko" cascade="persist,merge,save-update" class="pl.edu.uj.kognitywistyka.model.Stanowiska" column="Stanowisko"/>
   </class>
</hibernate-mapping>