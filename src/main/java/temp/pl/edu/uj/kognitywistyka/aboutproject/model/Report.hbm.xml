<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pl.edu.uj.kognitywistyka.aboutproject.model.Report" table="k_Raporty">
		<id name="reportId" column="Id">
			<generator class="increment"/>
		</id>
		<property name="title" column="Tytul"/>
		<property name="data" column="Data"/>
		<property name="content" column="Tresc"/>
   </class>
</hibernate-mapping>