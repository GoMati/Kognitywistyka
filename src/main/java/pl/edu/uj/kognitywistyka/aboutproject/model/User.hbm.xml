<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pl.edu.uj.kognitywistyka.visitor.model.User" table="k_Uzytkownicy">
		<id name="userId" column="Id">
			<generator class="increment"/>
		</id>
		<property name="login" column="Login"/>
		<property name="password" column="Haslo"/>
		<property name="email" column="Email"/>
		<property name="date" column="DataRejestracji"/>
		<property name="firstName" column="Imie"/>
		<property name="lastName" column="Nazwisko"/>
		<property name="TytulSpecjalizacja" column="TytulSpecjalizacja"/>
		<property name="NazwaUczelniLabor" column="NazwaUczelniLabor"/>
		<property name="Branza" column="Branza"/>
		<property name="Miejscowosc" column="Miejscowosc"/>
		<property name="Wojewodztwo" column="Wojewodztwo"/>
   </class>
</hibernate-mapping>