<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pl.edu.uj.kognitywistyka.aboutproject.model.Link" table="k_Linki">
		<id name="linkId" column="Id">
			<generator class="increment"/>
		</id>
		<property name="name" column="Naglowek"/>
		<property name="url" column="Url"/>
		<property name="description" column="Opis"/>
   </class>
</hibernate-mapping>